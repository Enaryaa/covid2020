{
  "name": "riot-config-example",
  "version": "1.0.0",
  "description": "Riot with riot.config.js",
  "private": true,
  "scripts": {
    "start": "npm run watch & npm run server",
    "build": "npx riot --config riot.config.js src/main.js",
    "watch": "npx riot --config riot.config.js -w src/main.js",
    "server": "serve"
  },
  "devDependencies": {
    "@riotjs/cli": "^4.0.3",
    "@riotjs/compiler": "^4.9.1",
    "serve": "^11.3.2"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "babel": "^6.23.0"
  }
}
